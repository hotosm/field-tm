# Timeseries data about projects
[no-cd]
timeseries:
  docker compose run --rm \
    api python /opt/stats/timeseries.py

# Get total feature mapped per project
[no-cd]
total-mapped:
  docker compose run --rm \
    api python /opt/stats/total_mapped.py
